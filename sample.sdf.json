{
  "version": "1.0",
  "meta": {
    "title": "Moving Circle"
  },
  "params": {
    "speed": 1.0
  },
  "globals": {
    "R": "0.3 + 0.05*sin(t*2)"
  },
  "shapes": [
    {
      "type": "circle",
      "op": "union",
      "x": "0.5*cos(t*params.speed)",
      "y": "0.2*sin(t)",
      "r": "R"
    },
    {
      "type": "box",
      "op": "subtract",
      "x": "0",
      "y": "0",
      "w": "0.2",
      "h": "0.2"
    }
  ],
  "output": {
    "edge": true
  }
}
